{
  "manifest_version": 1,
  "type": "WebApp",

  "meta": {
    "name": "Dota 2 Match Logger",
    "author": "Vasanth",
    "version": "1.0.0",
    "minimum-overwolf-version": "0.187.0",
    "description": "Logs Dota 2 match data and events for analysis using Overwolf GEP.",
    "dock_button_title": "Dota 2 Logger",
    "icon": "icon.png",
    "icon_gray": "icon_gray.png",
    "launcher_icon": "icon.ico",
    "window_icon": "icon.png"
  },

  "permissions": [
    "GameInfo",
    "GameEvents",
    "IO",
    "Microphone"
  ],

  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [7314]
    },
    "start_window": "index",
    "windows": {
      "index": {
        "file": "index.html",
        "show_in_taskbar": true,
        "resizable": true,
        "transparent": false,
        "size": {
          "width": 1000,
          "height": 700
        },
        "open_dev_tools": true
      }
    },
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": { "game_ids": [7314] },
        "start_minimized": false
      }
    ],
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000
    }
  }
}
